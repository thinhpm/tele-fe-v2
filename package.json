{
  "name": "apg-nuxt3-scrap-layer",
  "version": "0.0.1",
  "description": "",
  "author": "ARCHIPELAGO Inc.",
  "private": true,
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "dev:https": "cross-env CONFIG_FILE=\"local\" nuxt dev --host=0.0.0.0 --port=443 --https --ssl-cert ./xxx.com.pem --ssl-key ./xxx.com-key.pem",
    "dev": "cross-env CONFIG_FILE=\"local\" nuxt dev --host=0.0.0.0 --port=24670",
    "dev:dev": "cross-env CONFIG_FILE=\"dev\" nuxt dev --host=0.0.0.0 --port=24670",
    "dev:stg": "cross-env CONFIG_FILE=\"stg\" nuxt dev --host=0.0.0.0 --port=24670",
    "dev:prod": "cross-env CONFIG_FILE=\"prod\" nuxt dev --host=0.0.0.0 --port=24670",
    "build:dev": "cross-env CONFIG_FILE=\"dev\" nuxt build",
    "build:stg": "cross-env CONFIG_FILE=\"stg\" nuxt build",
    "build:prod": "cross-env CONFIG_FILE=\"prod\" nuxt build",
    "build:storybook": "storybook build",
    "start:dev": "cross-env CONFIG_FILE=\"dev\" nuxt start",
    "start:stg": "cross-env CONFIG_FILE=\"stg\" nuxt start",
    "start:prod": "cross-env CONFIG_FILE=\"prod\" nuxt start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --single-thread",
    "generate:dev": "cross-env CONFIG_FILE=\"dev\" nuxt generate",
    "generate:stg": "cross-env CONFIG_FILE=\"stg\" nuxt generate",
    "generate:prod": "cross-env CONFIG_FILE=\"prod\" nuxt generate",
    "preview": "cross-env CONFIG_FILE=\"local\" nuxt preview",
    "preview:dev": "cross-env CONFIG_FILE=\"dev\" nuxt preview",
    "preview:stg": "cross-env CONFIG_FILE=\"stg\" nuxt preview",
    "preview:prod": "cross-env CONFIG_FILE=\"prod\" nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:code": "eslint 'src/**/*.{js,ts,vue,}' --ignore-path .gitignore",
    "lintfix:code": "eslint 'src/**/*.{js,ts,vue}' --fix --ignore-path .gitignore",
    "lint:type": "tsc -p . --noEmit && vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "yarn lint:code",
    "*.scss": "yarn lint:style"
  },
  "devDependencies": {
    "@gtm-support/vue-gtm": "2.1.x",
    "@nuxt/devtools": "0.8.x",
    "@nuxtjs/eslint-config-typescript": "12.1.x",
    "@pinia/nuxt": "0.5.x",
    "@storybook/addon-essentials": "^7.4.2",
    "@storybook/addon-interactions": "^7.4.2",
    "@storybook/addon-links": "^7.4.2",
    "@storybook/blocks": "^7.4.2",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/vue3": "^7.4.2",
    "@storybook/vue3-vite": "^7.4.2",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "20.8.x",
    "@typescript-eslint/eslint-plugin": "6.8.x",
    "@typescript-eslint/parser": "6.8.x",
    "@vueuse/core": "10.5.x",
    "@vueuse/nuxt": "10.5.x",
    "cross-env": "7.0.x",
    "dayjs-nuxt": "2.0.x",
    "eslint": "8.51.x",
    "eslint-plugin-vue": "9.17.x",
    "happy-dom": "^12.9.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "minimist": "1.2.x",
    "nuxt": "3.7.4",
    "postcss-html": "1.5.x",
    "quick-lru": "7.0.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "1.69.x",
    "storybook": "^7.4.2",
    "storybook-addon-nuxt": "^1.3.3",
    "stylelint": "14.16.x",
    "stylelint-config-property-sort-order-smacss": "9.1.x",
    "stylelint-config-standard-scss": "6.1.x",
    "stylelint-config-standard-vue": "1.0.x",
    "swiper": "10.2.x",
    "typescript": "5.2.x",
    "vitest": "^0.34.6"
  },
  "dependencies": {}
}
